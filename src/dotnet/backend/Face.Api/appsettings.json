{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },

  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=FacePlatform;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "AiServices": {
    "FramePreprocess": {
      "BaseUrl": "http://localhost:8010",
      "TimeoutSeconds": 5
    },
    "ImageNormalize": {
      "BaseUrl": "http://localhost:8020",
      "TimeoutSeconds": 10
    },
    "FaceDetectPython": {
      "BaseUrl": "http://localhost:8030",
      "TimeoutSeconds": 10
    }
  },
  "Jwt": {
    "Key": "THIS_IS_DEMO_SECRET_KEY_CHANGE_IN_PRODUCTION_123456",
    "Issuer": "FacePlatform",
    "Audience": "FacePlatformClients",
    "ExpiresMinutes": 120
  },
